{
  "deno.enable": true,
  "deno.import_intellisense_autodiscovery": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "deno.import_intellisense_origins": {
    "https://deno.land": true
  },
  "files.exclude": {
    "node_modules/": true
  }
}
