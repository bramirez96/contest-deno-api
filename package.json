{
  "name": "ss-deno-be",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/bramirez96/deno-server-ss.git",
  "author": "Brandon Ramirez <bran.ramirez.don@gmail.com>",
  "license": "MIT",
  "scripts": {
    "latest": "yarn knex migrate:latest",
    "latest:test": "yarn cross-env NODE_ENV=testing knex migrate:latest",
    "rollback": "yarn knex migrate:rollback",
    "rollback:test": "yarn cross-env NODE_ENV=testing knex migrate:rollback",
    "seed": "yarn knex seed:run",
    "seed:test": "yarn cross-env NODE_ENV=testing knex seed:run",
    "reset": "yarn rollback && yarn latest && yarn seed",
    "reset:test": "yarn rollback:test && yarn latest:test && yarn seed:test",
    "make": "yarn knex migrate:make",
    "makeseed": "yarn knex seed:make"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "knex": "^0.21.15",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
